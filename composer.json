{
    "name": "rudra/cli",
    "description": "Cli Application",
    "type": "library",
    "keywords": [
        "rudra",
        "cli",
        "command-line"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagepard",
            "email": "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",    
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru",
        "issues": "https://github.com/Jagepard/Rudra-Cli/issues",    
        "source": "https://github.com/Jagepard/Rudra-Cli"    
    },
    "require": {
        "php": ">=8.3",
        "rudra/container": "self.version",
        "rudra/exception": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Rudra\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rudra\\Cli\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    },    
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}