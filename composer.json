{
    "name": "rudra/cli",
    "description": "Cli Application",
    "type": "library",
    "keywords": [
        "rudra",
        "cli",
        "command-line"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagepard",
            "email": "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru"
    },
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "rudra/docs": "dev-master",
        "rudra/exception": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Rudra\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rudra\\Cli\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "prefer-stable": true
    }
}
